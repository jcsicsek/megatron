module.exports.create = function() {
  var self = {   
    ustatementsummary: function(req, res) {
      res.render('manage/consumer/invoicesummary', { title: "Invoice | tabb.io" });
    },
    ustatementdetail: function(req, res) {
      res.render('manage/consumer/invoicedetail', { title: "Statements | tabb.io" });
    },
    upay: function(req, res) {
      res.render('manage/consumer/pay', { title: "Make a Payment | tabb.io" });
    }
  };
  return self;
}
